<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.noble.bikeshare"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ReserveActivity"
            android:label="@string/title_activity_reserve">
        </activity>
        <activity android:name=".OptionsActivity">
        </activity>
        <activity android:name=".OtherBikesActivity">
        </activity>
        <activity android:name=".DifferentBikeActivity">
        </activity>
    </application>

</manifest>
